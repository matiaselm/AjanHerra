<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matias.viewpagerwithtabs">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logocompressed2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logocompressed2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivityTabs"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".activities.InfoActivity" />
        <activity android:name=".activities.SettingsActivity" />
        <activity android:name=".activities.AddActivity"></activity>
    </application>

</manifest>